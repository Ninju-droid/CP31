#include <bits/stdc++.h>
using namespace std;

const int MOD = 1e9+7;
const int MAXN = 1e6 + 5;
#define ll long long
#define all(x) (x).begin(), (x).end()
#define allr(x) (x).rbegin(), (x).rend()
ll modpow(ll base, ll exp) {
    ll result = 1;
    base %= MOD;
    while (exp > 0) {
        if (exp & 1) result = (result * base) % MOD;
        base = (base * base) % MOD;
        exp >>= 1;
    }
    return result;
}
ll modinv(ll a){
    return modpow(a,MOD-2);
}
vector<ll> fact(MAXN);
void precomputeFact(){
    fact[0] = 1;
    for(int i=1; i<MAXN; i++){
        fact[i] = (fact[i-1]*i) % MOD;
    }
}
ll nCr(int n,int r){
    if(r>n || r<0) return 0;
    ll denom = (fact[r] * fact[n-r]) % MOD;
    return (fact[n]*modinv(denom)) % MOD;
}
    
bool func(ll m,vector<ll>&a,vector<ll>&b,ll n){
    ll cnt = 0 ; 
    for(ll i =1;i<=n;i++){
        if(b[i]>=cnt && a[i]>= m-cnt-1)cnt++;
    } 
    return cnt>=m;
}   

bool vanish(string s, ll i){
    ll len = s.size(); 
    ll j = 0 ; 
    while(i<len){
        if(s[i]<s[j])return false; 
        else if(s[i]==s[j]){
            i++; j++; 
        }
        else return true;
    }
    return true;
}

void fun(){
    ll n,k;cin>>n>>k; 
    string s;cin>>s; 
    
    ll i = n-1; 
    while(i>0){
        if(s[i]>s[0]){
            for(ll j=s.size()-1;j>=i;j--){
                s.pop_back(); 
            } 
        }
        else if(s[i]==s[0]){
            if(vanish(s,i)){
                for(ll j=s.size()-1;j>=i;j--)s.pop_back(); 
            }
        }
        i--; 
    }
    while(s.size()<k){
        s+=s; 
    }
    while(s.size()>k){
        s.pop_back(); 
    }
    cout<<s<<endl;
}

int main() {
#ifndef ONLINE_JUDGE
    freopen("Input.txt", "r", stdin);
    freopen("Output.txt", "w", stdout);
#endif  
   int t = 1;
   //cin>>t;
   while(t--)fun(); 
   return 0 ; 
}
