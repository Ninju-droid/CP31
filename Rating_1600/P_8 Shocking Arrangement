#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define all(x) (x).begin(), (x).end()
#define allr(x) (x).rbegin(), (x).rend()

void fun(){
    ll n;cin>>n; 
    vector<ll>pos , neg ,ans , z; 
    for(ll i=0;i<n;i++){
        ll val ; cin>>val; 
        if(val<0)neg.push_back(val); 
        else if(val>0) pos.push_back(val); 
        else z.push_back(0);
    } 
    n = pos.size() ; ll m = neg.size() ;
    if(n==0 && m==0){
        cout<<"NO"<<endl;
        return ;
    }else cout<<"YES"<<endl;
    sort(allr(pos));sort(all(neg));  
    ll curn = 0 , i=0 , j=0 ;
    while(i<n || j<m){
        if(j==m){
            while(i<n) ans.push_back(pos[i++]); 
        }else if(i==n){
            while(j<m) ans.push_back(neg[j++]);
        }else if(curn<0){
            while(curn<0 && i<n){
                ans.push_back(pos[i]);
                curn+=pos[i++];
            } 
        }else{
            while(curn>=0 && j<m){
                ans.push_back(neg[j]);
                curn+=neg[j++];
            } 
        }
    }
    for(auto it : z)ans.push_back(it);
    for(auto it : ans)cout<<it<<" "; 
    cout<<endl;
}

int main() {
#ifndef ONLINE_JUDGE
    freopen("Input.txt", "r", stdin);
    freopen("Output.txt", "w", stdout);
#endif  
   int t = 1;
   cin>>t;
   while(t--)fun(); 
   return 0 ; 
}
